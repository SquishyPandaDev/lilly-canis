    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "typescript-json-schema": "^0.43.0",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  }
